{
  "version": 2,
  "builds": [
    { "src": "convert.js", "use": "@now/node" },
    { "src": "index.html", "use": "@now/static" }
  ],
  "alias": ["csv-to-json"],
  "github": {
    "enabled": false
  },
  "routes": [{ "src": "/convert",  "methods": ["POST"], "dest": "/convert.js", "headers": { "cache-control": "s-maxage=43200, max-age=0" } },
    { "src": "/",  "dest": "/index.html", "headers": { "cache-control": "s-maxage=43200, max-age=0" } }]

}